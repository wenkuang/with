{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img src="data:image/png;base64," />
        <span class="sf-toolbar-status{% if 50 < collector.count %} sf-toolbar-status-yellow{% endif %}">{{ collector.count }}</span>
        {% if collector.count %}
            in {{ '%.0fms'|format(collector.duration) }}
        {% endif %}
    {% endset %}
    {% set text %}
        {% if collector.count %}
            <div class="sf-toolbar-info-piece">
                <b>Commands</b>
                <span>{{ collector.count }}</span>
            </div>
            <div class="sf-toolbar-info-piece">
                <b>Duration</b>
                <span>{{ '%.2fms'|format(collector.duration) }}</span>
            </div>
        {% endif %}
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}


{% block menu %}
<span class="label">
    <span class="icon"><img src="data:image/png;base64," /></span>
    <strong>git</strong>
    <span class="count">
        <span>{{ collector.count }}</span>
        <span>{{ '%.0fms'|format(collector.duration) }}</span>
    </span>
</span>
{% endblock %}

{% block panel %}
    <h2>Git calls</h2>
    {% for name, records in collector.data %}
        <h3>{{ name }}</h3>
        <p>{{ collector.count(name) }} commands in {{ '%.0fms'|format(collector.duration(name)) }}</p>
        <ul class="alt">
            {% for record in records %}
                <li {% if record.level  == 200 %}style="background-color: #ffffbb;" {% endif %}class="{{ loop.index is odd ? 'odd' : 'even' }}">
                    <div>
                        <pre><code>{{ record.message }}</code></pre>
                    </div>
                </li>
            {% endfor %}
        </ul>
    {% endfor %}
{% endblock %}
